model Channel {
    id        Int       @unique @default(autoincrement())
    createdAt DateTime? @default(now()) @db.Timestamp(0)
    updatedAt DateTime? @updatedAt
    deletedAt DateTime?

    title    String?
    username String  @unique

    favourite     Boolean?
    meta          Json?
    lastMessageId Int?
    allFetched    Boolean? @default(false)

    isFetchable Boolean? @default(false)
    blogs       Blog[]

    @@unique([username, deletedAt])
}
